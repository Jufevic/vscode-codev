{
    "name": "vscode-codev",
    "displayName": "CodeV Macro-plus language support",
    "description": "CodeV Macro-plus language support for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "CodeV",
        "CODE V"
    ],
    "contributes": {
        "languages": [{
            "id": "codev",
            "aliases": ["CodeV", "CODEV", "codev"],
            "extensions": ["seq","SEQ","int","INT"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "codev",
            "scopeName": "source.codev",
            "path": "./syntaxes/codev.tmLanguage.json"
        }]
    }
}