{
    "name": "vscode-codev",
    "displayName": "CodeV",
    "description": "CodeV Macro-plus language support for Visual Studio Code",
    "publisher": "heterophyllus",
    "version": "0.2.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "CodeV",
        "CODE V"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/heterophyllus/vscode-codev.git"
    },
    "icon": "images/icon.jpg",
    "license": "MIT",
    "contributes": {
        "languages": [
            {
                "id": "codev",
                "aliases": [
                    "codev",
                    "CodeV",
                    "CODEV",
                    "CODE V"
                ],
                "extensions": [
                    "seq",
                    "SEQ"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "codev",
                "scopeName": "source.codev",
                "path": "./syntaxes/codev.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "codev",
                "path": "./snippets/codev.json"
            }
        ]
    }
}
